{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.1.0.json",
    "width": 380,
    "height": 170,
    "title": {
      "text": ["Dairy Production by Region", "1964-2022"],
      "subtitle": "Dairy production over time",
      "subtitleColor": "gray",
      "subtitleFontStyle": "italic",
      "dy": -20
    },
    "data": {
      "url": "https://raw.githubusercontent.com/josepht777/FIT3179/refs/heads/main/Visualization2/1964-2022%20Dairy.csv",
      "format": {"type": "csv"}
    },
    "transform": [
    ],
    "encoding": {
      "x": {
        "field": "Year",
        "type": "ordinal",
        "axis": {"labelAngle": -30},
        "sort": [
          "1964-1974",
          "1975-1985",
          "1986-1996",
          "1997-2008",
          "2008-2022"
        ]
      },
      "y": {"field": "Number", "type": "quantitative"},
      "color": {"field": "Region"},
      "tooltip": [
        {"field": "Number", "type": "quantitative", "format": ","},
        {"field": "Region", "type": "nominal"}
      ]
    },
    "layer": [
      {
        "mark": {"type": "line", "tooltip": true, "point": {"filled": true}}
      }
    ],
    "config": {
      "style": {"cell": {"stroke": "transparent"}},
      "font": "Arial",
      "title": {"fontSize": 17}
    }
  }
  